{
  "CosmosDB": {
    "Uri": "https://your-account-name.documents.azure.com:443/",
    "PrimaryKey": "your-key-here",
    "DatabaseName": "PhotoGalleries"
  },
  "Storage": {
    "ConnectionString": "your-connection-string",
    "ImageProcessingQueueName": "images-to-process",
    "MessageBatchSize": "10",
    "MessageBatchVisibilityTimeoutMins": "5"
  },
  "Options": {
    "ZeroMessagesPollIntervalSeconds": "5"
  },
  "Logging": {
    "Path": "/your-logs-path",
    "MinimumLevel": "Verbose"
  }
}